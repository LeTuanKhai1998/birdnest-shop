{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "framework": "nextjs",
  "functions": {
    "frontend/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_APP_URL": "@next_public_app_url",
    "NEXTAUTH_URL": "@nextauth_url",
    "NEXTAUTH_SECRET": "@nextauth_secret",
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "GITHUB_CLIENT_ID": "@github_client_id",
    "GITHUB_CLIENT_SECRET": "@github_client_secret",
    "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY": "@stripe_publishable_key",
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "STRIPE_WEBHOOK_SECRET": "@stripe_webhook_secret",
    "UPLOADTHING_SECRET": "@uploadthing_secret",
    "UPLOADTHING_APP_ID": "@uploadthing_app_id",
    "RESEND_API_KEY": "@resend_api_key",
    "NEXT_PUBLIC_SENTRY_DSN": "@sentry_dsn",
    "NEXT_PUBLIC_ENABLE_OAUTH": "@enable_oauth",
    "NEXT_PUBLIC_ENABLE_STRIPE": "@enable_stripe",
    "NEXT_PUBLIC_ENABLE_FILE_UPLOAD": "@enable_file_upload",
    "NEXT_PUBLIC_ENABLE_EMAIL": "@enable_email",
    "NEXT_PUBLIC_MAINTENANCE_MODE": "@maintenance_mode"
  }
} 